{
  "name": "@mov-ai/mov-fe-lib-code-editor",
  "version": "1.1.4-1",
  "description": "Mov.ai's library for Code Editors",
  "keywords": [
    "frontend",
    "monaco editor",
    "code editor",
    "react.js"
  ],
  "homepage": "https://github.com/MOV-AI/frontend-npm-lib-code-editor#readme",
  "bugs": {
    "url": "https://github.com/MOV-AI/frontend-npm-lib-code-editor/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MOV-AI/frontend-npm-lib-code-editor.git"
  },
  "license": "ISC",
  "author": "MOV.AI",
  "exports": {
    ".": {
      "import": "esm/mov-code.js",
      "default": "cjs/mov-code.js"
    }
  },
  "main": "cjs/mov-code.js",
  "module": "esm/mov-code.js",
  "browser": "web/mov-code.js",
  "types": "types/mov-code.d.ts",
  "files": [
    "cjs",
    "web",
    "esm",
    "types"
  ],
  "scripts": {
    "build": "scripts types && scripts build",
    "build-storybook": "build-storybook",
    "buildDev": "webpack --mode=development --watch",
    "clean": "rm -rf node_modules",
    "coverage": "npm test -- --coverage",
    "depcheck": "npx depcheck",
    "fast": "scripts build",
    "lock": "mv ../../pnpm-workspace.yaml ../../pnpm-workspace.yaml.bak ; pnpm i ; mv ../../pnpm-workspace.yaml.bak ../../pnpm-workspace.yaml",
    "prepare": "husky",
    "storybook": "start-storybook -p 6006",
    "test": "echo no tests"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "package.json": "sort-package-json"
  },
  "dependencies": {
    "@codingame/monaco-jsonrpc": "^0.4.1",
    "monaco-editor": "^0.36.0",
    "monaco-editor-core": "^0.36.0",
    "normalize-url": "^8.0.1",
    "prop-types": "^15.7.2",
    "reconnecting-websocket": "^4.4.0",
    "vscode-json-languageservice": "^5.4.1",
    "vscode-languageserver": "^9.0.1",
    "vscode-uri": "^3.0.3"
  },
  "devDependencies": {
    "@codingame/monaco-languageclient": "^0.17.3",
    "@mov-ai/mov-fe-lib-core": "workspace:^1.2.4-1",
    "@mov-ai/mov-fe-lib-react": "workspace:^1.3.10-0",
    "@mov-ai/mov-fe-lib-scripts": "^0.6.8-1",
    "@mov-ai/mov-fe-lib-sub": "^2.3.2-1",
    "http-proxy-middleware": "^3.0.2",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "lodash": "^4.17.21",
    "prettier": "3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sort-package-json": "2.10.1"
  },
  "peerDependencies": {
    "@codingame/monaco-languageclient": "^0.17.3",
    "@mov-ai/mov-fe-lib-core": "workspace:^1.2.4-1",
    "@mov-ai/mov-fe-lib-react": "workspace:^1.3.10-0",
    "@mov-ai/mov-fe-lib-sub": "^2.3.2-1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "packageManager": "pnpm@9.11.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/mov-ai"
  },
  "cdn": {
    "vscode": "@codingame/monaco-languageclient/lib/vscode-compatibility"
  },
  "entry": {
    "editor-worker": "monaco-editor/esm/vs/editor/editor.worker.js",
    "main": "src/mov-code.js"
  },
  "external": {
    "lodash": "global _",
    "react": "global React",
    "@mov-ai/mov-fe-lib-core": "global Movai",
    "@mov-ai/mov-fe-lib-react": "global MovaiReact",
    "@mov-ai/mov-fe-lib-sub": "global ttySub"
  },
  "library": "MovaiCode"
}
